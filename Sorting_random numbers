#include<stdio.h>
#include<math.h>

FILE *fptr,*fptr1;
int n;

typedef struct array
{
int data;
struct array *next;
}*node;

node getnode()
{int a;
    node newn;
    newn=(node)malloc(sizeof(struct array));
    if(newn==NULL)
    {
        printf("error");
        exit(0);
    }
    fscanf(fptr,"%d",&a);
//printf("%d",a);
    newn->data=a;

    newn->next=NULL;
    return newn;

}
node insert(node head)
{node newn=getnode();
    if(head==NULL)
    {
        head=newn;

    }

    newn->next=head;
    head=newn;
    return head;
}
node sort(node head)
{
node cur,p;
int temp;
cur=head;
p=cur->next;
for(int j=0;j<=100;j++)
{
    p=cur->next;
    for(int i=0;i<=100;i++)
{
    if(cur->data>p->data)
    {
    temp=cur->data;
        cur->data=p->data;
        p->data=temp;

        p=p->next;
    }
    else
    {
   p=p->next;
    }

}
cur=cur->next;}
return head;
}
main()
{float start,end;

    node head=NULL;
    fptr=fopen("file90.txt","r");
    fptr1=fopen("file24.txt","w");//opening the file

for(int i=0;i<=100;i++)
{
    head=insert(head);

}

head=sort(head);

node cur=head;

for(int i=0;i<=100;i++)
{
    printf(" %d",cur->data);
    fprintf(fptr1," %d",cur->data);  //wr
    cur=cur->next;
}
fprintf(fptr1,"%d",end-start);

fclose(fptr);
fclose(fptr1);
}

